rule "Primo VE - Lds09"
## Side B titles for Sound recordings
## Display: n/a

  when
    MARCRECORD AND
    MARC.control."007"(0-1) equals "s" AND
    MARC.control."007"(5-6) matches "s|m" AND
    MARC."245" has any "c"
  then
    set TEMP"1" to MARC."245" subfields "c"
    create pnx."display"."lds09" with TEMP"1"
end